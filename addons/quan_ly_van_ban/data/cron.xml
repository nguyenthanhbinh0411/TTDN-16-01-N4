<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_van_ban_den_progress" model="ir.cron">
            <field name="name">Cập nhật trạng thái văn bản đến</field>
            <field name="model_id" ref="model_van_ban_den"/>
            <field name="state">code</field>
            <field name="code">model._cron_update_progress()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
            <field name="active">True</field>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="ir_cron_yeu_cau_kh_progress" model="ir.cron">
            <field name="name">Cập nhật trạng thái yêu cầu/ khiếu nại</field>
            <field name="model_id" ref="model_yeu_cau_khach_hang"/>
            <field name="state">code</field>
            <field name="code">model._cron_update_progress()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
            <field name="active">True</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
    </data>
</odoo>
